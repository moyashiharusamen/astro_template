{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint --fix .scss",
    "eslint": "eslint --ext .ts --ignore-path .gitignore .",
    "eslint:fix": "eslint --fix --ext .ts,.astro --ignore-path .gitignore .",
    "prettier:write": "yarn prettier:write:ts && yarn prettier:write:astro",
    "prettier:write:ts": "prettier --write 'src/**/*.ts'",
    "prettier:write:astro": "prettier --write 'src/**/*.astro'",
    "prettier:check": "yarn prettier:check:ts && yarn prettier:check:astro",
    "prettier:check:ts": "prettier --check 'src/**/*.ts'",
    "prettier:check:astro": "prettier --check 'src/**/*.astro'",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@acab/reset.css": "^0.5.3",
    "astro": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "astro-compress": "^1.1.28",
    "eslint": "^8.32.0",
    "eslint-plugin-astro": "^0.23.0",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.8.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "uuidv4": "^6.2.13"
  }
}
